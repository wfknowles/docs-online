
<div class="container" style="    margin-top: 15px;">
  <div class="breadcrumbs">Sites &gt; Filter &gt; <%= @sticky.author %> , <%= @sticky.id %></div>
  <div class="paper-trail">Last Modified by
    <% if @version.whodunnit && @version.whodunnit != 'Guest' %>
        <% user = User.find_by_id(@version.whodunnit) %>
        <% if user %>
            <%= user.name %>
        <% end %>
    <% else %>
        Guest
    <% end %>
    on <%= l(@version.created_at, format: "%-d.%m.%Y %H:%M:%S %Z") %></div>
</div>
</div>


<div class="container">
  <div class="page-header form-group" style="margin-top: 10px;">
    <%= link_to 'Edit Records', edit_sticky_path(@sticky) , :class=>"btn btn-custom pull-right" , :style=>"margin-left:10px;"%> <%= link_to 'Back', stickies_path , :class=>"btn btn-custom pull-right" , :style=>"margin-left:10px;"%>
    <h1><%= @sticky.author %></h1>
  </div>
  <div class="form-group">

    <div class="form-group info-group">
      <p>
        <strong>Note:</strong>
        <%= @sticky.note %>
      </p>

      <p>
        <strong>Author:</strong>
        <%= @sticky.author %>
      </p>

      <p>
        <strong>Category:</strong>
        <%= @sticky.category %>
      </p>

      <p>
        <strong>Timestamp:</strong>
        <%= @sticky.timestamp %>
      </p>

      <p>
        <strong>Hide sticky:</strong>
        <%= @sticky.hide_sticky %>
      </p>
    </div>
  </div>
</div>