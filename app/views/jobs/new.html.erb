<div class="container" style="    margin-top: 15px;">
  <div class="breadcrumbs">Jobs &gt; <%=link_to 'Expiring Credentials Notification' ,jobs_path ,:class=>'link-to-field'%></div>


  <div class="paper-trail">Last Modified by
  <% if @version.whodunnit && @version.whodunnit != 'Guest' %>
      <% user = User.find_by_id(@version.whodunnit) %>
      <% if user %>
          <%= user.name %>
      <% end %>
  <% else %>
      Guest
  <% end %>
  on <%= l(@version.created_at, format: "%-d.%m.%Y %H:%M:%S %Z") %></div>
</div>

<div class="container">
  <div class="page-header form-group">
    <%= link_to 'EDIT', jobs_path, :class=>"btn btn-custom pull-right" %>
    <h3>Expiring Credentials Notification</h3>
  </div>

  <div class="form-group">
    <%= render 'form',job:@job %>
  </div>

</div>