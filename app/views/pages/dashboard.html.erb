<div class="col-md-2">
  <div class="gutter">
    <div id="shift-notes">
      <%if @stickies!=nil%>
       <%@stickies.each do |sticky|%>
          <div class="draggable ui-draggable ui-draggable-handle">
            <span class="glyphicon glyphicon-remove-circle"></span>
            <div class="shift-note-date"><%=sticky.timestamp %></div>
            <div class="shift-note-author"><%=sticky.author %>,<%=sticky.category %></div>
            <div class="shift-note"><%=sticky.note%></div>
          </div>
      <% end %>
      <% end %>
      <script>
        function onAdd(id){
            $.getScript('/events/sticky/?mid='+id, function() {});
        }
    </script>

    <div class="form-group">
      <a id="add-note" class="" onclick="onAdd(<%=@mid %>)" ><span class="glyphicon glyphicon-plus"></span>Add Note</a>
    </div>
  </div>
